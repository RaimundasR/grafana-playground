{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "11" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.x" }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["nginx", "prometheus", "stub_status"],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "constant",
        "label": "Job",
        "query": "nginx",
        "current": { "text": "nginx", "value": "nginx" },
        "hide": 2
      },
      {
        "name": "instance",
        "type": "query",
        "label": "Instance",
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 1,
        "query": "label_values(nginx_connections_active{job=\"$job\"}, instance)",
        "includeAll": true,
        "multi": true,
        "current": { "text": "All", "value": ["$__all"] }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "NGINX (Prometheus: stub_status)",
  "uid": "",
  "version": 1,
  "weekStart": "",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests / sec",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "sum by (instance) (rate(nginx_http_requests_total{job=\"$job\", instance=~\"$instance\"}[1m]))",
          "legendFormat": "{{instance}}",
          "range": true,
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Active connections",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "sum by (instance) (nginx_connections_active{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": "{{instance}}",
          "range": true,
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Connections: reading / writing / waiting",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (instance) (nginx_connections_reading{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": "reading {{instance}}",
          "range": true,
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (instance) (nginx_connections_writing{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": "writing {{instance}}",
          "range": true,
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (instance) (nginx_connections_waiting{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": "waiting {{instance}}",
          "range": true,
          "refId": "C"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Connections accepted / sec",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (instance) (rate(nginx_connections_accepted{job=\"$job\", instance=~\"$instance\"}[1m]))",
          "legendFormat": "{{instance}}",
          "range": true,
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Connections handled / sec",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (instance) (rate(nginx_connections_handled{job=\"$job\", instance=~\"$instance\"}[1m]))",
          "legendFormat": "{{instance}}",
          "range": true,
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Connection drops (1 - handled / accepted)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 24 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "clamp_min(1 - (sum by (instance) (rate(nginx_connections_handled{job=\\\"$job\\\", instance=~\\\"$instance\\\"}[5m])) / sum by (instance) (rate(nginx_connections_accepted{job=\\\"$job\\\", instance=~\\\"$instance\\\"}[5m]))), 0)",
          "legendFormat": "{{instance}}",
          "range": true,
          "refId": "A"
        }
      ]
    }
  ]
}
